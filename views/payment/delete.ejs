<button type="button" class="btn btn-outline-dark" data-toggle="modal" data-target="#paymentDelete<%= pm.checkNumber %>"><i class="fa fa-trash"></i></button>
<!-- The Modal -->
<div class="modal" id="paymentDelete<%= pm.checkNumber %>" data-backdrop="false">
    <div class="modal-dialog modal-md modal-dialog-scrollable">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="blackFont">Delete Cheque Number</h4>
            </div>
            <!-- Modal body -->
            <div class="modal-body">
                Do you want to delete this cheque number?
                <!-- Modal footer -->
                <form action="/payment/del/<%= pm.checkNumber %>" method="POST">
                    <button type="button"  class="btn btn-success" id="canDel" data-dismiss="modal">No</button>
                    <button type="submit" class="btn btn-danger">Yes</button>
                </form>
            </div>
        </div>
    </div>
</div>